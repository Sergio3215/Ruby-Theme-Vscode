	"tokenColors": [
		{
			"name": "Const Function declaration",
			"scope": [
				"storage.type",
			],
			"settings": {
			  "foreground": "#c486fd", // Cambia este color según tu preferencia
			  "fontStyle": "bold italic" // Opcional: hace que las constantes estén en negrita
			}
		},
		{
			"name": "Object key literal",
			"scope": [
  			  "meta.object-literal.key",
  			],
			"settings": {
			  "foreground": "#eb6629" // Cambia el color de las propiedades de un objeto
			}
		},
		{
			"name": "Object key when is call",
			"scope": [
			  "variable.other.property",
